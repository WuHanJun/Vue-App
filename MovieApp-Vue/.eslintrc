{
    "root": true,
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
    },
    "env": {
      "node": true,
      "browser": true
    },
    "plugins": [
      'html'
    ],
    "rules": {
        "semi": 1,
        "no-unused-vars": 1
    }
}
